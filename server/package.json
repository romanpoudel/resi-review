{
  "name": "resi_review",
  "version": "1.0.0",
  "description": "api for a house review app",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "make:migration": "knex --knexfile=src/knexFile.ts migrate:make",
    "migrate": "knex --knexfile=src/knexFile.ts migrate:latest --verbose",
    "rollback": "knex --knexfile=src/knexFile.ts migrate:rollback --verbose",
    "make:seeder": "knex --knexfile=src/knexFile.ts seed:make",
    "seed": "knex --knexfile=src/knexFile.ts seed:run --verbose"
  },
  "keywords": [],
  "author": "Roman Poudel",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/camelize": "^1.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/to-snake-case": "^1.0.2",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "camelize": "^1.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "to-snake-case": "^1.0.0",
    "winston": "^3.11.0"
  }
}
